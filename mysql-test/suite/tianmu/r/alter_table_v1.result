2	6	NULL
3	4	NULL
4	9	NULL
5	10	NULL
6	1	NULL
7	12	NULL
8	12	NULL
alter table t1 order by bandid,payoutid;
Warnings:
Warning	1105	ORDER BY ignored as there is a user-defined clustered index in the table 't1'
select * from t1;
