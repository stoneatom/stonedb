#
#  WEEKOFYEAR() function
# Author: syw
#
DROP DATABASE IF EXISTS weekofyear_function;
CREATE DATABASE weekofyear_function;
USE weekofyear_function;
CREATE TABLE t1
(
t1_DATE DATE,
t1_TIMESTAMP TIMESTAMP,
t1_DATETIME DATETIME
)ENGINE=tianmu;
INSERT INTO t1 VALUES('1991-05-30', '1999-10-28 01:00:50', '2020-12-11 01:08:59');
INSERT INTO t1 VALUES('2991-10-13', '1972-11-22 11:19:52', '2002-02-21 05:28:37');
SELECT WEEKOFYEAR('2020-10-12') FROM t1 LIMIT 1;
WEEKOFYEAR('2020-10-12')
42
SELECT WEEKOFYEAR('2020-05-22') FROM t1 LIMIT 1;
WEEKOFYEAR('2020-05-22')
21
SELECT WEEKOFYEAR('2020-02-13 12:13:14') FROM t1 LIMIT 1;
WEEKOFYEAR('2020-02-13 12:13:14')
7
SELECT WEEKOFYEAR('12:13:14') FROM t1 LIMIT 1;
WEEKOFYEAR('12:13:14')
NULL
Warnings:
Warning	1292	Incorrect datetime value: '12:13:14'
SELECT t1_DATE, WEEKOFYEAR(t1_DATE) FROM t1 ORDER BY 1;
t1_DATE	WEEKOFYEAR(t1_DATE)
1991-05-30	22
2991-10-13	41
SELECT t1_DATETIME, WEEKOFYEAR(t1_DATETIME) FROM t1 ORDER BY 1;
t1_DATETIME	WEEKOFYEAR(t1_DATETIME)
2002-02-21 05:28:37	8
2020-12-11 01:08:59	50
SELECT t1_TIMESTAMP, WEEKOFYEAR(t1_TIMESTAMP) FROM t1 ORDER BY 1;
t1_TIMESTAMP	WEEKOFYEAR(t1_TIMESTAMP)
1972-11-22 11:19:52	47
1999-10-28 01:00:50	43
DROP DATABASE weekofyear_function;
