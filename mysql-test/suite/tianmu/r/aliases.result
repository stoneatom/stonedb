CREATE TABLE t2(col1 INT, col2 TEXT)ENGINE=Tianmu;
INSERT INTO t2 VALUES(1, repeat('s',20)),(2, repeat('o',20)),(3, 'sss'),(4, 'ooo');
CREATE TABLE t3(col1 INT, col2 DATETIME)ENGINE=Tianmu;
