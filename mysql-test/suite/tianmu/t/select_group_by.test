#
# Test basic select...group by
#
--disable_warnings
DROP DATABASE IF EXISTS db1;
--enable_warnings

CREATE DATABASE db1;
USE db1;

CREATE TABLE t1
(
 t1_tinyint TINYINT DEFAULT 0,
 t1_int INT DEFAULT NULL,
 t1_bigint BIGINT,
 t1_double DOUBLE,
 t1_float FLOAT,
 t1_blob BLOB,
 t1_text TEXT,
 t1_char CHAR(1),
 t1_varchar VARCHAR(255) DEFAULT 'hello world!',
 t1_datetime DATETIME
)ENGINE=tianmu;

